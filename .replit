run = "npm run dev -- --host 0.0.0.0"

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "npm run build && npm run preview -- --host 0.0.0.0"]

[[ports]]
localPort = 5173
externalPort = 80

[env]
PATH = "/home/runner/$REPL_SLUG/node_modules/.bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"